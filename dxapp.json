{
  "name": "eggd_archive_system",
  "title": "DNAnexus archival system for East GLH",
  "summary": "App to archive and unarchive projects and files on DNAnexus",
  "dxapi": "1.0.0",
  "version": "1.0.0",
  "inputSpec": [
    {
      "name": "file_to_archive",
      "label": "files ready to be archived",
      "class": "array:file",
      "optional": true,
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "file_to_unarchive",
      "label": "files ready to be unarchived",
      "class": "array:file",
      "optional": true,
      "patterns": [
        "*"
      ],
      "help": ""
    }
  ],
  "outputSpec": [
    {
      "name": "archived_log",
      "label": "File that monitors archival state and completion",
      "class": "array:file",
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "unarchived_log",
      "label": "File that monitors unarchived state and completion",
      "class": "array:file",
      "patterns": [
        "*"
      ],
      "help": ""
    }
  ],
  "runSpec": {
    "timeoutPolicy": {
      "*": {
        "hours": 48
      }
    },
    "interpreter": "python3",
    "file": "src/eggd_archive_system.py",
    "distribution": "Ubuntu",
    "release": "16.04",
    "version": "1"
  },
  "access": {
    "network": [
      "*"
    ]
  },
  "regionalOptions": {
    "aws:us-east-1": {
      "systemRequirements": {
        "*": {
          "instanceType": "mem3_ssd1_v2_x16"
        }
      }
    }
  }
}
